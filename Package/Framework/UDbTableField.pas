{ TTableFieldsDef }

procedure TTableFieldsDef.CreateMemberObjects;
const OPNAME = 'TTableFieldsDef.CreateMemberObjects';
begin
  inherited;
  try
    FFields := TStringList.Create;
  except on E: Exception do HandleError(E,OPNAME) end;
end;

procedure TTableFieldsDef.DestroyMemberObjects;
const OPNAME = 'TTableFieldsDef.DestroyMemberObjects';
begin
  inherited;
  try
    FFields.Free;
  except on E: Exception do HandleError(E,OPNAME) end;
end;

procedure TTableFieldsDef.AddTableFieldDef(ATableFieldDef: TTableFieldDef);
const OPNAME = 'TTableFieldsDef.AddTableFieldDef';
begin
  try
   if(ATableFieldDef <> nil) then
     FFields.AddObject(ATableFieldDef.TableName,ATableFieldDef);
  except on E: Exception do HandleError(E,OPNAME) end;
end;

function TTableFieldsDef.GetFieldByIndex(AIndex: integer): TAbstractTableFieldDef;
const OPNAME = 'TTableFieldsDef.GetFieldByIndex';
begin
  Result := nil;
  try
   if(AIndex >= 0) and (AIndex < FFields.Count) then
     Result := TAbstractTableFieldDef(FFields.Objects[AIndex]);
  except on E: Exception do HandleError(E,OPNAME) end;
end;

function TTableFieldsDef.GetFieldByName(AName: string): TAbstractTableFieldDef;
const OPNAME = 'TTableFieldsDef.GetFieldByName';
var
  LIndex: integer;
begin
  Result := nil;
  try
    for LIndex := 0 to FFields.Count -1 do
    begin
      if(UpperCase(FieldByIndex[LIndex].FieldName) = UpperCase(AName)) then
      begin
        Result := FieldByIndex[LIndex];
      end;
    end;
  except on E: Exception do HandleError(E,OPNAME) end;
end;

function TTableFieldsDef.GetTableName: string;
const OPNAME = 'TTableFieldsDef.GetTableName';
begin
  Result := '';
  try
    Result := FTableName;
  except on E: Exception do HandleError(E,OPNAME) end;
end;

function TTableFieldsDef.GetFieldsCount: integer;
const OPNAME = 'TTableFieldsDef.GetFieldsCount';
begin
  Result := NullInteger;
  try
    Result := FFields.Count;
  except on E: Exception do HandleError(E,OPNAME) end;
end;

function TTableFieldsDef.GetCreateTableFields: string;
const OPNAME = 'TTableFieldsDef.GetCreateTableFields';
var
  LIndex: integer;
  LFieldDef: string;
  LDbTableField: TAbstractTableFieldDef;
begin
  Result := '';
  try
    for LIndex := 0 to FFields.Count - 1 do
    begin
      LDbTableField := FieldByIndex[LIndex];
      LFieldDef := LDbTableField.FieldName + ' '+ LDbTableField.FieldType;
      if(LDbTableField.FieldLength > 0) then
        LFieldDef := LFieldDef + ' ('+ IntToStr(LDbTableField.FieldLength) + ')';
      if(Trim(LFieldDef) <> '') then
      begin
        if(LIndex < (FFields.Count - 1)) then
          Result := Result + ' ' + LFieldDef + ',' + #10#13
        else
          Result := Result + ' ' + LFieldDef ;
      end;
    end;
//    Result := FTableName;
  except on E: Exception do HandleError(E,OPNAME) end;
end;

