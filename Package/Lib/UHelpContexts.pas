function SetControlHelpContext(AControl: TControl; AHelpContext: integer): boolean;
function SetAllControlsHelpContext(AControl: TControl; AHelpContext: integer): boolean;

implementation

function SetControlHelpContext(AControl: TControl; AHelpContext: integer): boolean;

begin
  Result := False;
  If Assigned(AControl) Then
  begin
    AControl.HelpType := htContext;
    AControl.HelpContext := AHelpContext;
    Result := True;
  end;
end;

function SetAllControlsHelpContext(AControl: TControl; AHelpContext: integer): boolean;
var
  LCount: integer;
  LControl: TControl;
begin
  Result :=  SetControlHelpContext(AControl,AHelpContext);
  for LCount := 0 to AControl.ComponentCount - 1 do
  begin
    if(AControl.Components[LCount] is TControl) then
    begin
      LControl := TControl(AControl.Components[LCount]);
      Result :=  Result and SetControlHelpContext(LControl,AControl.HelpContext);
    end;
  end;
end;

end.

