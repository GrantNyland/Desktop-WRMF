  TTableFieldDef = class (TAbstractTableFieldDef)
  protected
    FTableName   : string;
    FFieldName   : string;
    FFieldType   : string;
    FFieldLength : integer;
    procedure CreateMemberObjects; override;
    procedure DestroyMemberObjects; override;
    function GetTableName: string; override;
    function GetFieldName : string; override;
    function GetFieldType : string; override;
    function GetFieldLength : integer; override;
  public
    function Initialise: boolean; override;
    procedure SetMemberVariables ( ATableName : string; AFieldName : string;
                                   AFieldType : string; AFieldLength : integer );
  end;

  TTableFieldsDef = class (TAbstractTableFieldsDef)
  protected
    FTableName   : string;
    FFields: TStringList;
    procedure CreateMemberObjects; override;
    procedure DestroyMemberObjects; override;
    function GetTableName   : string; override;
    function GetFieldByIndex(AIndex: integer):TAbstractTableFieldDef;override;
    function GetFieldByName(AName: string):TAbstractTableFieldDef;override;
    function GetCreateTableFields: string;override;
    function GetFieldsCount: integer;override;
  public
    procedure AddTableFieldDef(ATableFieldDef:TTableFieldDef);
  end;

  TTableFieldDefItemAddFunction = procedure (
  ATableName        : string;
  AFieldNames       : String;
  AFieldType        : string;
  AFieldLength      : integer ) of object;

procedure LoadTableFieldsData ( AAdd : TTableFieldDefItemAddFunction );
procedure LoadMoreTableFieldsData ( AAdd : TTableFieldDefItemAddFunction );

implementation
uses
  SysUtils,
  UConstants,
  UErrorHandlingOperations;


{ TTableFieldDef }

procedure TTableFieldDef.CreateMemberObjects;
const OPNAME = 'TTableFieldDef.CreateMemberObjects';
begin
  inherited;
  try
    FTableName       := '';
    FFieldLength     := 0;
    FFieldName      := '';
    FFieldType      := '';
  except on E: Exception do HandleError(E,OPNAME) end;
end;

procedure TTableFieldDef.DestroyMemberObjects;
const OPNAME = 'TTableFieldDef.DestroyMemberObjects';
begin
  inherited;
  try
  except on E: Exception do HandleError(E,OPNAME) end;
end;

function TTableFieldDef.Initialise: boolean;
const OPNAME = 'TTableFieldDef.Initialise';
begin
  Result := inherited Initialise;
  try
    FTableName       := '';
    FFieldLength     := 0;
    FFieldType       := '';
    FFieldName       := '';
    Result := True;
  except on E: Exception do HandleError(E,OPNAME) end;
end;

procedure TTableFieldDef.SetMemberVariables ( ATableName : string; AFieldName : string;
                                              AFieldType : string; AFieldLength : integer );
const OPNAME = 'TTableFieldDef.SetMemberVariables';
begin
  try
    FTableName := ATableName;
    FFieldName := AFieldName;
    FFieldType := AFieldType;
    FFieldLength := AFieldLength;
  except on E: Exception do HandleError(E,OPNAME) end;
end;

